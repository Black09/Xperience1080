        <texturedown>buttons/spinctrl/arrow-down-nf.png</texturedown>
        <textureupfocus>buttons/spinctrl/arrow-up-fo.png</textureupfocus>
        <texturedownfocus>buttons/spinctrl/arrow-down-fo.png</texturedownfocus>
