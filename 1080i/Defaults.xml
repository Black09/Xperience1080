<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <default type="label">
        <posx>0</posx>
        <posy>0</posy>
        <label>-</label>
        <height>30</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>Font-Condensed-S32</font>
        <textcolor>white</textcolor>
        <focusedcolor>white</focusedcolor>
        <selectedcolor>white</selectedcolor>
        <disabledcolor>black</disabledcolor>
        <scroll>true</scroll>
    </default>
    <default type="image">
        <posx>0</posx>
        <posy>0</posy>
        <width>-</width>
        <height>-</height>
        <align>center</align>
        <aligny>center</aligny>
        <texture>-</texture>
        <aspectratio>stretch</aspectratio>
    </default>
    <default type="button">
        <posx>0</posx>
        <posy>0</posy>
        <width>150</width>
        <height>30</height>
        <align>center</align>
        <aligny>center</aligny>
        <font>Font-Condensed-S26-B</font>
        <label>-</label>
