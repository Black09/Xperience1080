        <spinheight>60</spinheight>
        <textcolor>grey</textcolor>
        <focusedcolor>blue</focusedcolor>
        <disabledcolor>black</disabledcolor>
        <textoffsetx>0</textoffsetx>
        <align>left</align>
        <aligny>center</aligny>
        <include condition="!Skin.HasSetting(SpinControl.Default)">SpinControlReversed</include>
        <include condition="Skin.HasSetting(SpinControl.Default)">SpinControlDefault</include>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="spincontrol">
        <posx>0</posx>
        <posy>0</posy>
